<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext https://softwaredesign.foundation/xsd/liquibase/dbchangelog-ext.xsd">

    <changeSet id="as_02"
               author="Lucas Reeh">
        <ext:alterSchema schemaName="my_schema">
            <ext:renameTo schema="my_new_schema"></ext:renameTo>
        </ext:alterSchema>
    </changeSet>

    <changeSet id="as_03"
               author="Lucas Reeh">
        <ext:alterSchema schemaName="my_schema">
            <ext:ownerTo owner="my_owner"></ext:ownerTo>
        </ext:alterSchema>
    </changeSet>

</databaseChangeLog>